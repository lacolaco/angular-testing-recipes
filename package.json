{
  "name": "angular-testing-recipes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng test",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && stylelint 'src/**/*.css'",
    "lint:fix": "ng lint --fix && stylelint 'src/**/*.css' --fix",
    "docs:json": "compodoc --silent -p ./tsconfig.json -e json -d .storybook/",
    "prestorybook": "yarn docs:json",
    "storybook": "ng run angular-testing-recipes:storybook",
    "prestorybook:build": "yarn prestorybook",
    "storybook:build": "ng run angular-testing-recipes:build-storybook",
    "vistest": "./vistest/vistest.mjs",
    "vistest:check": "yarn vistest --check",
    "vistest:update": "yarn vistest --update"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.3",
    "@angular/cdk": "^14.0.3",
    "@angular/common": "^14.0.3",
    "@angular/compiler": "^14.0.3",
    "@angular/core": "^14.0.3",
    "@angular/forms": "^14.0.3",
    "@angular/platform-browser": "^14.0.3",
    "@angular/platform-browser-dynamic": "^14.0.3",
    "@angular/router": "^14.0.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular-eslint/builder": "14.0.0",
    "@angular-eslint/eslint-plugin": "14.0.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0",
    "@angular-eslint/schematics": "14.0.0",
    "@angular-eslint/template-parser": "14.0.0",
    "@angular/cli": "^14.0.3",
    "@angular/compiler-cli": "^14.0.3",
    "@angular/elements": "^14.0.3",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/angular": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@testing-library/angular": "^12.0.0",
    "@testing-library/jasmine-dom": "^1.2.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^2.3.1",
    "puppeteer": "^13.0.0",
    "reg-cli": "^0.17.0",
    "storycap": "^3.0.4",
    "stylelint": "^14.9.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "~4.7.4",
    "zx": "^7.0.0"
  }
}
