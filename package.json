    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.4",
    "jest-preset-angular": "^11.0.0",
    "prettier": "^2.3.1",
    "puppeteer": "^10.1.0",
    "reg-cli": "^0.17.0",
    "storycap": "^3.0.4",
